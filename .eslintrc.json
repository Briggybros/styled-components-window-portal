{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/prop-types": "off"
    },
    "env": {
        "es6": true,
        "browser": true
    }
}